[
  {
    "description":"这是一个带header信息的post请求",
    "request":{
      "uri":"/post/with/header",
      "method":"post",
      "headers":{
        "content-type":"application/json"
      },
      "json":{
        "name":"lisi",
        "sex":"female"
      }
    },
    "response":{
      "json":{
        "lisi":"very good!",
        "status":1
      }
    }
  }
]